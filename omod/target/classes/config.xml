<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>billingmigration</id>
	<name>billingmigration Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.billingmigration</package>
	<author>RBCEMR</author>
	<description>
		Migrate existing billing data to new created table
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/billingmigration/update.rdf</updateURL>
	<require_version>1.6.6</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.billingmigration.billingmigrationActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.billingmigration.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		billingmigration.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with billingmigration. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

